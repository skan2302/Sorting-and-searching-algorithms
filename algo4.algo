PROCEDURE swap(VAR x:INTEGER,VAR y:INTEGER)
VAR
    z : INTEGER;
BEGIN
    z:=x;
    x:=y;
    y:=z;
END

PROCEDURE insertionSort (VAR tab: ARRAY_OF INTEGER)
VAR
    i,j,tmp:INTEGER;
    
BEGIN
    i,j:=0;
    FOR i FROM 0 TO n-1 STEP 1  DO
        tmp:=tab[i];
        j:=i-1
         WHILE (j>=1) and (tmp<tab[j]) DO
            tab[j+1]:=tab[j];
            j:=j-1
         END_WHILE 
        tab[j+1]:=tmp
    END_FOR
END
